{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "description": "This is the TypeScript implementation for ASAPP challenge",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc --p tsconfig.json && npm run copy-templates",
    "clean": "rimraf ./lib",
    "copy-templates": "copyfiles --up 1 \"./lib\"",
    "start": "npm run build && node ./lib/index.js",
    "test": "npm run test"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/sqlite3": "^3.1.7",
    "copyfiles": "^2.1.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.2.2"
  },
  "author": "Martin Battaglino",
  "license": "ISC"
}
